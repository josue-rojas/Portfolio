<div id="slideView">
  <div class="container slideBox">
    <div id="myCarousel" class="carousel slide" data-ride="carousel">
      <!-- Indicators -->
      <ol class="carousel-indicators">
        <% if(data){for(j = 0 ; j < data.length; j++){ %>
          <li data-target="#myCarousel" data-slide-to="<%= j %>" class=<% if(j==0){ %>"active" <% } %>></li>
          <% } }%>
      </ol>

      <!-- Wrapper for slides -->
      <div class="carousel-inner">
      <% if(data){for(l = 0 ; l < data.length; l++){ %>
        <div class="item <% if(l==0){ %>active <% } %>" id="item1">
          <div id="top<%= l %>" class="invi" onclick=""></div>
          <div class="container" id="slideCon<%= l+1 %>">
            <div class="jumbotron" id="slideJumbo<%= l+1 %>">
              <a href="<%= data[l].url %>">
                <img src="images/github_logo.png" class="containerImage" id="slide<%= l+1 %>" onmouseout="gitImage(<%= l+1 %>, false)" onmouseover="gitImage(<%= l+1 %>)"/>
              </a>
              <h2><%= data[l].title %></h2>
              <p class="lead"><%= data[l].languages %></p>
              <p><%= data[l].info %></p>
            </div>
          </div>
        </div>
        <% } }%>
      </div>

      <!-- Left and right controls -->
      <a class="left carousel-control" href="#myCarousel" data-slide="prev">
        <span class="glyphicon glyphicon-chevron-left"></span>
        <span class="sr-only">Previous</span>
      </a>
      <a class="right carousel-control" href="#myCarousel" data-slide="next">
        <span class="glyphicon glyphicon-chevron-right"></span>
        <span class="sr-only">Next</span>
      </a>
    </div>
    <center><img src="github_logo.png" class="picLink"/><img src="github_logo.png" class="picLink"/><img src="github_logo.png" class="picLink"/></center>
  </div>

</div>
